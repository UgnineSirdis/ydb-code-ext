{
  "name": "vibedb",
  "displayName": "VibeDB",
  "description": "Extension for YDB developers that helps to do everyday job",
  "version": "0.0.1",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/UgnineSirdis/vibe-db-extension"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vibedb.helloWorld",
        "title": "Hi all"
      },
      {
        "command": "vibedb.createLocalYdb",
        "title": "New local-ydb",
        "icon": "$(add)"
      }
    ],
    "views": {
      "debug": [
        {
          "id": "local-ydb",
          "name": "Local YDB",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vibedb.createLocalYdb",
          "when": "view == local-ydb",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
